{
    "name": "@ssromp/ui",
    "version": "0.1.0",
    "author": {
        "name": "Nicolas GORAJ"
    },
    "description": "ui library made of SolidJs components",
    "scripts": {
        "start": "vite",
        "storybook": "storybook dev -p 6006",
        "storybook:build": "storybook build",
        "chromatic": "npx chromatic --exit-zero-on-changes",
        "lint": "npx eslint --max-warnings 0 .",
        "format": "npx prettier --write --config ../../.prettierrc .",
        "build": "vite build",
        "prepublishOnly": "pnpm run build"
    },
    "type": "module",
    "private": false,
    "main": "dist/main.js",
    "types": "dist/main.d.ts",
    "files": [
        "dist"
    ],
    "sideEffects": [
        "**/*.css"
    ],
    "publishConfig": {
        "access": "public"
    },
    "license": "GPL-3.0-or-later",
    "peerDependencies": {
        "solid-js": "^1.8.11"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "catalog:chromatic-official",
        "@eslint/js": "catalog:eslint-official",
        "@storybook/addon-essentials": "catalog:storybook-official",
        "@storybook/addon-interactions": "catalog:storybook-official",
        "@storybook/addon-links": "catalog:storybook-official",
        "@storybook/blocks": "catalog:storybook-official",
        "@storybook/docs-tools": "catalog:storybook-official",
        "@storybook/test": "catalog:storybook-official",
        "@types/node": "catalog:types-community",
        "@typescript-eslint/parser": "catalog:eslint-community",
        "chromatic": "catalog:chromatic-official",
        "eslint-plugin-solid": "catalog:solid-community",
        "glob": "catalog:utilities-community",
        "globals": "catalog:eslint-official",
        "solid-devtools": "catalog:solid-official",
        "solid-js": "catalog:solid-official",
        "storybook": "catalog:storybook-official",
        "storybook-solidjs": "catalog:storybook-official",
        "storybook-solidjs-vite": "catalog:storybook-official",
        "typescript": "catalog:typescript-official",
        "vite": "catalog:vite-official",
        "vite-plugin-dts": "catalog:vite-community",
        "vite-plugin-lib-inject-css": "catalog:vite-unsure",
        "vite-plugin-solid": "catalog:solid-official"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/sassy-stack-repositories/one-more-pull.git"
    },
    "keywords": [
        "vite",
        "solidjs",
        "ui",
        "library",
        "components"
    ],
    "bugs": {
        "url": "https://github.com/sassy-stack-repositories/one-more-pull/issues"
    },
    "homepage": "https://github.com/sassy-stack-repositories/one-more-pull#readme"
}
